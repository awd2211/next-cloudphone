# 管理后台环境变量配置示例
# 复制此文件为 .env 并填入实际值

# ===== API 配置 =====
VITE_API_BASE_URL=http://localhost:30000/api
VITE_API_TIMEOUT=10000  # 毫秒

# ===== WebSocket 配置 =====
VITE_WS_URL=ws://localhost:30000

# ===== 应用配置 =====
VITE_APP_TITLE=云手机管理后台
VITE_APP_VERSION=1.0.0

# ===== 功能开关 =====
VITE_ENABLE_MOCK=false
VITE_ENABLE_DEBUG=true
VITE_ENABLE_PWA=false

# ===== 上传配置 =====
VITE_UPLOAD_MAX_SIZE=209715200  # 200MB (字节)
VITE_UPLOAD_CHUNK_SIZE=5242880  # 5MB (字节)

# ===== 分页配置 =====
VITE_PAGE_SIZE=10
VITE_PAGE_SIZE_OPTIONS=10,20,50,100

# ===== 图表配置 =====
VITE_CHART_THEME=light  # light | dark

# ===== 开发配置 =====
VITE_DEV_SERVER_PORT=5173
VITE_DEV_SERVER_HOST=0.0.0.0
