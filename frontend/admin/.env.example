# 云手机平台 - 管理后台环境变量配置

# API 基础 URL
VITE_API_BASE_URL=http://localhost:3000/api

# 应用标题
VITE_APP_TITLE=云手机平台管理后台

# 应用版本
VITE_APP_VERSION=1.0.0

# 是否启用 Mock 数据
VITE_USE_MOCK=false

# WebSocket URL
VITE_WS_URL=ws://localhost:3000

# 文件上传大小限制（MB）
VITE_MAX_FILE_SIZE=100

# 是否启用调试模式
VITE_DEBUG=false

# 日志级别 (debug | info | warn | error)
VITE_LOG_LEVEL=info

# 超时时间（毫秒）
VITE_REQUEST_TIMEOUT=10000

# 是否显示性能监控
VITE_SHOW_PERFORMANCE=false
