# 用户端环境变量配置示例
# 复制此文件为 .env 并填入实际值

# ===== API 配置 =====
VITE_API_BASE_URL=http://localhost:30000/api
VITE_API_TIMEOUT=10000  # 毫秒

# ===== WebSocket 配置 =====
VITE_WS_URL=ws://localhost:30000

# ===== 应用配置 =====
VITE_APP_TITLE=云手机用户端
VITE_APP_VERSION=1.0.0

# ===== 功能开关 =====
VITE_ENABLE_MOCK=false
VITE_ENABLE_DEBUG=true
VITE_ENABLE_PWA=false

# ===== WebRTC 配置 =====
VITE_WEBRTC_SERVER_URL=http://localhost:30009
VITE_WEBRTC_ICE_SERVERS=stun:stun.l.google.com:19302

# ===== 支付配置 =====
VITE_ENABLE_WECHAT_PAY=false
VITE_ENABLE_ALIPAY=false
VITE_ENABLE_BALANCE_PAY=true

# ===== 上传配置 =====
VITE_UPLOAD_MAX_SIZE=10485760  # 10MB (字节)

# ===== 分页配置 =====
VITE_PAGE_SIZE=12
VITE_PAGE_SIZE_OPTIONS=12,24,48

# ===== 社交登录配置 =====
# Google OAuth
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Facebook OAuth
VITE_FACEBOOK_APP_ID=your-facebook-app-id

# Twitter (X) OAuth 2.0
VITE_TWITTER_CLIENT_ID=your-twitter-client-id

# ===== 开发配置 =====
VITE_DEV_SERVER_PORT=5174
VITE_DEV_SERVER_HOST=0.0.0.0
