{
  "name": "cloudphone-platform",
  "version": "1.0.0",
  "description": "Cloud Phone Platform",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "test": "pnpm -r test"
  },
  "pnpm": {
    "overrides": {
      "typeorm": "0.3.20"
    },
    "peerDependencyRules": {
      "ignoreMissing": ["@nestjs/core", "@nestjs/common"],
      "allowAny": ["@nestjs/*"]
    }
  }
}
