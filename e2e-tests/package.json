{
  "name": "@cloudphone/e2e-tests",
  "version": "1.0.0",
  "description": "End-to-End Integration Tests for Cloud Phone Platform",
  "private": true,
  "scripts": {
    "test": "jest --runInBand --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --runInBand --forceExit",
    "test:user": "jest --testPathPattern=user --runInBand",
    "test:device": "jest --testPathPattern=device --runInBand",
    "test:billing": "jest --testPathPattern=billing --runInBand",
    "pretest": "node helpers/wait-for-services.js"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
