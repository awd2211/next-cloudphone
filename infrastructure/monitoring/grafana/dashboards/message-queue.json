{
  "title": "Cloud Phone - Message Queue",
  "tags": [
    "cloudphone",
    "rabbitmq",
    "queue"
  ],
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 0,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "type": "timeseries",
      "title": "Message Publish Rate",
      "targets": [
        {
          "expr": "rate(rabbitmq_channel_messages_published_total[5m])",
          "legendFormat": "{{queue}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "mps"
        }
      }
    },
    {
      "id": 2,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "type": "timeseries",
      "title": "Message Consume Rate",
      "targets": [
        {
          "expr": "rate(rabbitmq_channel_messages_delivered_total[5m])",
          "legendFormat": "{{queue}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "mps"
        }
      }
    },
    {
      "id": 3,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "type": "timeseries",
      "title": "Queue Depth",
      "targets": [
        {
          "expr": "rabbitmq_queue_messages_ready",
          "legendFormat": "{{queue}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "id": 4,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "type": "timeseries",
      "title": "DLX Messages",
      "targets": [
        {
          "expr": "rabbitmq_queue_messages{queue=~\".*dlx.*\"}",
          "legendFormat": "{{queue}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    }
  ]
}
