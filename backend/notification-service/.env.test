# 测试环境配置
NODE_ENV=test
PORT=30006

# PostgreSQL 测试数据库
DB_HOST=localhost
DB_PORT=5433
DB_USERNAME=test_user
DB_PASSWORD=test_password
DB_DATABASE=cloudphone_notification_test

# Redis 测试缓存
REDIS_HOST=localhost
REDIS_PORT=6380

# RabbitMQ 测试消息队列
RABBITMQ_URL=amqp://test_admin:test_password@localhost:5673/cloudphone_test

# JWT (与开发环境一致)
JWT_SECRET=test-secret-key-for-integration-tests
JWT_EXPIRES_IN=7d

# SMTP (使用 mock)
SMTP_HOST=localhost
SMTP_PORT=1025
SMTP_USER=test@example.com
SMTP_PASS=test_password
SMTP_FROM=noreply@cloudphone-test.com

# WebSocket
WS_PORT=30006

# 日志级别
LOG_LEVEL=error

# Consul (集成测试中禁用)
CONSUL_ENABLED=false
